<included>
    <appender name="gslog.silentAppender" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>${gslog.pattern}</pattern>
        </encoder>
        <filter class="ch.qos.logback.classic.filter.LevelFilter">
            <level>ERROR</level>
            <onMatch>DENY</onMatch>
            <onMismatch>DENY</onMismatch>
        </filter>
    </appender>

</included>




